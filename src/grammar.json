{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "javascript",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SYMBOL",
            "name": "string"
          },
          {
            "type": "SYMBOL",
            "name": "comment"
          },
          {
            "type": "SYMBOL",
            "name": "keyword"
          },
          {
            "type": "SYMBOL",
            "name": "regex"
          },
          {
            "type": "SYMBOL",
            "name": "identifier"
          }
        ]
      }
    },
    "comment": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_multi_comment"
        },
        {
          "type": "SYMBOL",
          "name": "_single_comment"
        }
      ]
    },
    "_multi_comment": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "/*"
        },
        {
          "type": "TOKEN",
          "content": {
            "type": "PATTERN",
            "value": "([^*]|\\*[^/])*"
          }
        },
        {
          "type": "STRING",
          "value": "*/"
        }
      ]
    },
    "_single_comment": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "\\/\\/.*\\n"
      }
    },
    "keyword": {
      "type": "TOKEN",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "STRING",
            "value": "as"
          },
          {
            "type": "STRING",
            "value": "break"
          },
          {
            "type": "STRING",
            "value": "case"
          },
          {
            "type": "STRING",
            "value": "class"
          },
          {
            "type": "STRING",
            "value": "const"
          },
          {
            "type": "STRING",
            "value": "continue"
          },
          {
            "type": "STRING",
            "value": "default"
          },
          {
            "type": "STRING",
            "value": "delete"
          },
          {
            "type": "STRING",
            "value": "else"
          },
          {
            "type": "STRING",
            "value": "export"
          },
          {
            "type": "STRING",
            "value": "for"
          },
          {
            "type": "STRING",
            "value": "from"
          },
          {
            "type": "STRING",
            "value": "function"
          },
          {
            "type": "STRING",
            "value": "if"
          },
          {
            "type": "STRING",
            "value": "import"
          },
          {
            "type": "STRING",
            "value": "in"
          },
          {
            "type": "STRING",
            "value": "let"
          },
          {
            "type": "STRING",
            "value": "new"
          },
          {
            "type": "STRING",
            "value": "of"
          },
          {
            "type": "STRING",
            "value": "return"
          },
          {
            "type": "STRING",
            "value": "switch"
          },
          {
            "type": "STRING",
            "value": "var"
          },
          {
            "type": "STRING",
            "value": "while"
          }
        ]
      }
    },
    "identifier": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "[a-zA-Z0-9_]+"
      }
    },
    "regex": {
      "type": "TOKEN",
      "content": {
        "type": "PREC",
        "value": 1,
        "content": {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "/"
            },
            {
              "type": "CHOICE",
              "members": [
                {
                  "type": "PATTERN",
                  "value": "[^*/\\n\\\\]"
                },
                {
                  "type": "PATTERN",
                  "value": "\\\\."
                }
              ]
            },
            {
              "type": "REPEAT",
              "content": {
                "type": "CHOICE",
                "members": [
                  {
                    "type": "PATTERN",
                    "value": "[^/\\n\\\\]"
                  },
                  {
                    "type": "PATTERN",
                    "value": "\\\\."
                  }
                ]
              }
            },
            {
              "type": "STRING",
              "value": "/"
            },
            {
              "type": "CHOICE",
              "members": [
                {
                  "type": "PATTERN",
                  "value": "[a-zA-Z]+"
                },
                {
                  "type": "BLANK"
                }
              ]
            }
          ]
        }
      }
    },
    "string": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_single_string"
        },
        {
          "type": "SYMBOL",
          "name": "_double_string"
        },
        {
          "type": "SYMBOL",
          "name": "_template_string"
        }
      ]
    },
    "_single_string": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "'"
        },
        {
          "type": "TOKEN",
          "content": {
            "type": "PATTERN",
            "value": "[^']*"
          }
        },
        {
          "type": "STRING",
          "value": "'"
        }
      ]
    },
    "_double_string": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "\""
        },
        {
          "type": "TOKEN",
          "content": {
            "type": "PATTERN",
            "value": "([^\"]|\\\\.)*"
          }
        },
        {
          "type": "STRING",
          "value": "\""
        }
      ]
    },
    "_template_string": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "`"
        },
        {
          "type": "TOKEN",
          "content": {
            "type": "PATTERN",
            "value": "[^`]*"
          }
        },
        {
          "type": "STRING",
          "value": "`"
        }
      ]
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}