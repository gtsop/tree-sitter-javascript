{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "javascript",
  "rules": {
    "source_file": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_js_context"
        },
        {
          "type": "BLANK"
        }
      ]
    },
    "_js_context": {
      "type": "REPEAT1",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SYMBOL",
            "name": "string"
          },
          {
            "type": "SYMBOL",
            "name": "comment"
          },
          {
            "type": "SYMBOL",
            "name": "keyword"
          },
          {
            "type": "SYMBOL",
            "name": "regex"
          },
          {
            "type": "SYMBOL",
            "name": "identifier"
          },
          {
            "type": "SYMBOL",
            "name": "_full_jsx"
          }
        ]
      }
    },
    "comment": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_multi_comment"
        },
        {
          "type": "SYMBOL",
          "name": "_single_comment"
        }
      ]
    },
    "_multi_comment": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "/*"
        },
        {
          "type": "TOKEN",
          "content": {
            "type": "PATTERN",
            "value": "([^*]|\\*[^/])*"
          }
        },
        {
          "type": "STRING",
          "value": "*/"
        }
      ]
    },
    "_single_comment": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "\\/\\/.*\\n"
      }
    },
    "keyword": {
      "type": "TOKEN",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "STRING",
            "value": "as"
          },
          {
            "type": "STRING",
            "value": "break"
          },
          {
            "type": "STRING",
            "value": "case"
          },
          {
            "type": "STRING",
            "value": "class"
          },
          {
            "type": "STRING",
            "value": "const"
          },
          {
            "type": "STRING",
            "value": "continue"
          },
          {
            "type": "STRING",
            "value": "default"
          },
          {
            "type": "STRING",
            "value": "delete"
          },
          {
            "type": "STRING",
            "value": "else"
          },
          {
            "type": "STRING",
            "value": "export"
          },
          {
            "type": "STRING",
            "value": "extends"
          },
          {
            "type": "STRING",
            "value": "for"
          },
          {
            "type": "STRING",
            "value": "from"
          },
          {
            "type": "STRING",
            "value": "function"
          },
          {
            "type": "STRING",
            "value": "if"
          },
          {
            "type": "STRING",
            "value": "import"
          },
          {
            "type": "STRING",
            "value": "in"
          },
          {
            "type": "STRING",
            "value": "interface"
          },
          {
            "type": "STRING",
            "value": "keyof"
          },
          {
            "type": "STRING",
            "value": "let"
          },
          {
            "type": "STRING",
            "value": "new"
          },
          {
            "type": "STRING",
            "value": "of"
          },
          {
            "type": "STRING",
            "value": "return"
          },
          {
            "type": "STRING",
            "value": "switch"
          },
          {
            "type": "STRING",
            "value": "type"
          },
          {
            "type": "STRING",
            "value": "var"
          },
          {
            "type": "STRING",
            "value": "while"
          }
        ]
      }
    },
    "identifier": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "[a-zA-Z0-9_]+"
      }
    },
    "regex": {
      "type": "TOKEN",
      "content": {
        "type": "PREC",
        "value": 1,
        "content": {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "/"
            },
            {
              "type": "CHOICE",
              "members": [
                {
                  "type": "PATTERN",
                  "value": "[^*/\\n\\\\]"
                },
                {
                  "type": "PATTERN",
                  "value": "\\\\."
                }
              ]
            },
            {
              "type": "REPEAT",
              "content": {
                "type": "CHOICE",
                "members": [
                  {
                    "type": "PATTERN",
                    "value": "[^/\\n\\\\]"
                  },
                  {
                    "type": "PATTERN",
                    "value": "\\\\."
                  }
                ]
              }
            },
            {
              "type": "STRING",
              "value": "/"
            },
            {
              "type": "CHOICE",
              "members": [
                {
                  "type": "PATTERN",
                  "value": "[a-zA-Z]+"
                },
                {
                  "type": "BLANK"
                }
              ]
            }
          ]
        }
      }
    },
    "string": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_single_string"
        },
        {
          "type": "SYMBOL",
          "name": "_double_string"
        },
        {
          "type": "SYMBOL",
          "name": "_template_string"
        }
      ]
    },
    "_single_string": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "'"
        },
        {
          "type": "TOKEN",
          "content": {
            "type": "PATTERN",
            "value": "[^']*"
          }
        },
        {
          "type": "STRING",
          "value": "'"
        }
      ]
    },
    "_double_string": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "\""
        },
        {
          "type": "TOKEN",
          "content": {
            "type": "PATTERN",
            "value": "([^\"]|\\\\.)*"
          }
        },
        {
          "type": "STRING",
          "value": "\""
        }
      ]
    },
    "_template_string": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "`"
        },
        {
          "type": "TOKEN",
          "content": {
            "type": "PATTERN",
            "value": "[^`]*"
          }
        },
        {
          "type": "STRING",
          "value": "`"
        }
      ]
    },
    "jsx_start": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "<"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "PATTERN",
              "value": "[a-zA-Z]*"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "REPEAT",
              "content": {
                "type": "SEQ",
                "members": [
                  {
                    "type": "SYMBOL",
                    "name": "attribute_name"
                  },
                  {
                    "type": "CHOICE",
                    "members": [
                      {
                        "type": "SEQ",
                        "members": [
                          {
                            "type": "STRING",
                            "value": "="
                          },
                          {
                            "type": "SYMBOL",
                            "name": "attribute_value"
                          }
                        ]
                      },
                      {
                        "type": "BLANK"
                      }
                    ]
                  }
                ]
              }
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "STRING",
          "value": ">"
        }
      ]
    },
    "_full_jsx": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SEQ",
          "members": [
            {
              "type": "SYMBOL",
              "name": "jsx_start"
            },
            {
              "type": "CHOICE",
              "members": [
                {
                  "type": "REPEAT",
                  "content": {
                    "type": "CHOICE",
                    "members": [
                      {
                        "type": "SYMBOL",
                        "name": "text"
                      },
                      {
                        "type": "SYMBOL",
                        "name": "_full_jsx"
                      },
                      {
                        "type": "SYMBOL",
                        "name": "comment"
                      }
                    ]
                  }
                },
                {
                  "type": "BLANK"
                }
              ]
            },
            {
              "type": "SYMBOL",
              "name": "jsx_end"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "jsx_self"
        }
      ]
    },
    "jsx_self": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "<"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "PATTERN",
              "value": "[a-zA-Z]*"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "REPEAT",
              "content": {
                "type": "SEQ",
                "members": [
                  {
                    "type": "SYMBOL",
                    "name": "attribute_name"
                  },
                  {
                    "type": "CHOICE",
                    "members": [
                      {
                        "type": "SEQ",
                        "members": [
                          {
                            "type": "STRING",
                            "value": "="
                          },
                          {
                            "type": "SYMBOL",
                            "name": "attribute_value"
                          }
                        ]
                      },
                      {
                        "type": "BLANK"
                      }
                    ]
                  }
                ]
              }
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "STRING",
          "value": "/>"
        }
      ]
    },
    "jsx_end": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "</"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "PATTERN",
              "value": "[a-zA-Z]+"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "STRING",
          "value": ">"
        }
      ]
    },
    "attribute_name": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "[a-zA-Z]+"
      }
    },
    "attribute_value": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "\""
            },
            {
              "type": "PATTERN",
              "value": "[a-z]+"
            },
            {
              "type": "STRING",
              "value": "\""
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "jsx_context"
        }
      ]
    },
    "jsx_context": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "{"
        },
        {
          "type": "SYMBOL",
          "name": "_js_context"
        },
        {
          "type": "STRING",
          "value": "}"
        }
      ]
    },
    "text": {
      "type": "PATTERN",
      "value": "[^<]+"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [
    [
      "jsx_start",
      "jsx_self"
    ]
  ],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}